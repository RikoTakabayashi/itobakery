<%= form_for @pan do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name, :size => 140 %>
    <%= f.label :kinds %>
    <%= f.text_field :kinds, :size => 140 %>
    <%= f.label :about %>
    <%= f.text_field :about, :size => 140 %>
    <%= f.label :price %>
    <%= f.text_field :price, :size => 140 %>
    <%= f.submit "編集する" %>
  <% end %>